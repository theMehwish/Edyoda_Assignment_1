   "source": [
    "a = input(\"Type a word:\")\n",
    "\n",
    "def reverse_string(a):\n",
    "    b = \"\"\n",
    "    for i in a:\n",
    "        b = i + b\n",
    "    return b\n",
    "print(reverse_string(a))"
